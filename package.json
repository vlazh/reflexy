{
  "name": "reflexy",
  "version": "3.41.7",
  "description": "Flexbox layout react components",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "author": "VZH",
  "license": "MIT",
  "homepage": "https://github.com/vlazh/reflexy",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/reflexy"
  },
  "bugs": {
    "url": "https://github.com/vlazh/reflexy/issues"
  },
  "keywords": [
    "react",
    "flexbox",
    "flex",
    "layout"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles package.json README.md LICENSE CHANGELOG.md ./dist/",
    "copy:css": "copyfiles ./src/**/*.css --up 1 ./dist/",
    "build-ts": "tsc --project tsconfig.json",
    "build": "npm run clean && npm run build-ts && npm run copy:css",
    "build-to-tutoron": "tsc --project tsconfig.json --outDir '../../../tutoron/node_modules/reflexy/' && copyfiles ./src/**/*.css --up 1 '../../../tutoron/node_modules/reflexy/'",
    "build-to-react-components": "tsc --project tsconfig.json --outDir '../react-components/node_modules/reflexy/'",
    "build-to-styled-components": "tsc --project tsconfig.json --outDir '../styled-components/node_modules/reflexy/'",
    "build-to-player": "tsc --project ./tsconfig.json --outDir '../../../player/node_modules/reflexy' && copyfiles ./src/**/*.css --up 1 '../../../player/node_modules/reflexy'",
    "patch-publish": "npm run build && npm version patch && npm run copy:configs && npm publish ./dist/ && git push --follow-tags",
    "minor-publish": "npm run build && npm version minor && npm run copy:configs && npm publish ./dist/ && git push --follow-tags",
    "version": "conventional-changelog -i CHANGELOG.md -s && git add CHANGELOG.md",
    "changelog": "conventional-changelog --config ./changelog.config.js -i CHANGELOG.md -s"
  },
  "devDependencies": {
    "@js-toolkit/configs": "^3.84.2",
    "@mui/styles": "^5.5.1",
    "@mui/system": "^5.5.1",
    "@types/css-modules": "^1.0.2",
    "@types/react": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "conventional-changelog-cli": "^2.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "stylelint": "^14.5.3",
    "stylelint-config-css-modules": "^4.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0"
  },
  "peerDependencies": {
    "react": ">= 16.8.x"
  }
}
