{
  "name": "reflexy",
  "version": "2.4.1",
  "description": "Flexbox layout react components",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "contributors": [
    {
      "name": "Vladimir Zhukov",
      "url": "http://github.com/vlazh"
    }
  ],
  "homepage": "https://github.com/vlazh/reflexy",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/reflexy"
  },
  "bugs": {
    "url": "https://github.com/vlazh/reflexy/issues"
  },
  "keywords": [
    "react",
    "flexbox",
    "flex",
    "layout"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && tsc --project ./lib/tsconfig.json && cross-env NODE_ENV=production webpack --config webpack.config.ts",
    "build-ts": "npm run clean && tsc --project ./lib/tsconfig.json",
    "build-ts-to-tutoron": "npm run clean && tsc --project ./lib/tsconfig.json --outDir '../tutoron/node_modules/reflexy/dist/'",
    "build-ts-to-pwa": "npm run clean && tsc --project ./lib/tsconfig.json --outDir '../pwa/node_modules/reflexy/dist/'",
    "build-ts-to-react-components": "npm run clean && tsc --project ./lib/tsconfig.json --outDir '../@vzh-react-components/node_modules/reflexy/dist/'",
    "build-wp": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.ts",
    "prepublishOnly": "npm run clean && npm run build",
    "patch-publish": "npm version patch && npm publish && git push --follow-tags",
    "minor-publish": "npm version minor && npm publish && git push --follow-tags",
    "major-publish": "npm version major && npm publish && git push --follow-tags"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/css-modules": "^1.0.0",
    "@types/react": "^16.7.18",
    "@types/webpack-merge": "^4.1.3",
    "@vzh/configs": "^3.9.1",
    "autoprefixer": "^9.4.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "csstype": "^2.6.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-typescript": "^0.14.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-icss-values": "^2.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.1",
    "postcss-preset-env": "^6.5.0",
    "prettier": "^1.15.3",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^2.6.3",
    "stylelint": "^9.9.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-prettier": "^1.0.6",
    "ts-node": "^7.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^21.0.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "classnames": ">= 2.2",
    "@vzh/ts-types": ">= 0.4"
  },
  "peerDependencies": {
    "react": ">= 16.x"
  }
}
