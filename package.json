{
  "name": "reflexy",
  "version": "1.15.1",
  "description": "Flexbox layout react components",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && tsc --project ./tsconfig.json && webpack --config webpack.config.babel.js",
    "build-ts": "npm run clean && tsc --project ./tsconfig.json",
    "build-wp": "npm run clean && webpack --config webpack.config.babel.js",
    "prepublishOnly": "npm run clean && npm run build",
    "patch-publish": "npm version patch && npm publish && git push --follow-tags",
    "minor-publish": "npm version minor && npm publish && git push --follow-tags",
    "major-publish": "npm version major && npm publish && git push --follow-tags"
  },
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "contributors": [
    {
      "name": "Vladimir Zhukov",
      "url": "http://github.com/vlazh"
    }
  ],
  "homepage": "https://github.com/vlazh/reflexy",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/reflexy"
  },
  "bugs": {
    "url": "https://github.com/vlazh/reflexy/issues"
  },
  "keywords": [
    "react",
    "flexbox",
    "flex",
    "layout"
  ],
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/css-modules": "^1.0.0",
    "@types/react": "^16.4.6",
    "@vzh/configs": "^1.18.0",
    "autoprefixer": "^8.6.5",
    "awesome-typescript-loader": "^5.2.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "postcss-loader": "^2.1.5",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^2.6.2",
    "stylelint": "^9.3.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^3.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.3",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-consistent-codestyle": "^1.13.2",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "peerDependencies": {
    "react": ">= 15.x"
  }
}
