{
  "name": "reflexy",
  "version": "2.12.0",
  "description": "Flexbox layout react components",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "contributors": [
    {
      "name": "Vladimir Zhukov",
      "url": "http://github.com/vlazh"
    }
  ],
  "homepage": "https://github.com/vlazh/reflexy",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/reflexy"
  },
  "bugs": {
    "url": "https://github.com/vlazh/reflexy/issues"
  },
  "keywords": [
    "react",
    "flexbox",
    "flex",
    "layout"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles package.json README.md LICENSE ./dist/",
    "copy:css": "copyfiles ./lib/**/*.css --up 2 ./dist/",
    "build-ts": "npm run clean && tsc --project ./lib/tsconfig.json",
    "build": "npm run build-ts && npm run copy:css && npm run copy:configs",
    "build-ts-to-tutoron": "npm run clean && tsc --project ./lib/tsconfig.json --outDir '/home/vladimir/dev/sources/tutoron/node_modules/reflexy/'",
    "build-ts-to-pwa": "npm run clean && tsc --project ./lib/tsconfig.json --outDir '/home/vladimir/dev/sources/pwa/node_modules/reflexy/'",
    "build-ts-to-react-components": "npm run clean && tsc --project ./lib/tsconfig.json --outDir '../react-components/node_modules/reflexy/'",
    "patch-publish": "npm version patch && npm run build && npm publish ./dist/ && git push --follow-tags",
    "minor-publish": "npm version minor && npm run build && npm publish ./dist/ && git push --follow-tags",
    "major-publish": "npm version major && npm run build && npm publish ./dist/ && git push --follow-tags"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@types/classnames": "^2.2.7",
    "@types/css-modules": "^1.0.0",
    "@types/react": "^16.8.6",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "@vzh/configs": "^3.16.1",
    "babel-eslint": "^10.0.1",
    "copyfiles": "^2.1.0",
    "csstype": "^2.6.2",
    "eslint": "^5.15.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.4.0",
    "prettier": "^1.16.4",
    "prettier-stylelint": "^0.4.2",
    "react": "^16.8.3",
    "rimraf": "^2.6.3",
    "stylelint": "^9.10.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-prettier": "^1.0.6",
    "typescript": "^3.3.3333",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "classnames": ">= 2.2"
  },
  "peerDependencies": {
    "react": ">= 16.8.x"
  }
}
